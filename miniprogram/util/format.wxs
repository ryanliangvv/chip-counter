function calcAllBuyin(buyinList) {
  var total = 0;  
  for (var index = 0; index < buyinList.length; index++) {
    var e = buyinList[index];
    total = total + e.buyinCount || 0    
  }  
  return total
}

function formatChipsCount(number1) {
   // 处理负数，取绝对值来做格式化，最后再加回负号
   var number = Math.abs(number1);
   var isNegative = number1 < 0;
   var formattedNumber = number;
   var suffix = '';

   if (number < 1000) {
     return (isNegative ? '-' : '') + number.toString();  // 小于1000，直接返回数字，带上负号（如果有）
   } else {
     // 判断是否为万级别，超过1000000就显示 "w"
     if (formattedNumber >= 1000000) {
       formattedNumber = formattedNumber / 10000;  // 除以10000，转换为"w"
       suffix = 'w';
     } else {
       formattedNumber = formattedNumber / 1000;   // 否则，除以1000，转换为"k"
       suffix = 'k';
     }
     // 使用 toString 方法确保仅保留有效小数部分
     return (isNegative ? '-' : '') + formattedNumber.toString() + suffix;
   }
}

